<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.p5_anynote">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.P5_AnyNote">
        <activity android:name=".NotesAsLabel"
            android:theme="@style/Theme.P5_AnyNote.NoActionBar"></activity>
        <activity
            android:name=".Trash"
            android:theme="@style/Theme.P5_AnyNote.NoActionBar"></activity>
        <activity
            android:name=".Archive"
            android:theme="@style/Theme.P5_AnyNote.NoActionBar" />
        <activity
            android:name=".Notes"
            android:theme="@style/Theme.P5_AnyNote.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Base"
            android:theme="@style/Theme.P5_AnyNote.NoActionBar"></activity>
    </application>

</manifest>